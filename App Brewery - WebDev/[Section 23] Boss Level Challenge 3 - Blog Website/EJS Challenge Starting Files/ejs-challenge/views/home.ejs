<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= content %></p>
<% array.forEach(element=> { %>
<h1><%= element.title %></h1>
<p>
  <%= _.truncate(element.body, {length: 100}) %> <% if (element.body.length >
  100) {%>
  <a href="posts/<%= element.title %>">Read More</a>
  <% } %>
</p>

<% }) %> <%- include("partials/footer") -%>
